        <%= link_to"ホーム", home_index_path, class: "header-title"%>
      </li>
      <li class="list">
        <i class="fas fa-comment"></i>
        <%= link_to "チャットルーム", chat_rooms_path, data: { turbo_method: :post }, class: "header-title"%>
